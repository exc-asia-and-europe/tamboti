                                                <xf:label>+</xf:label>
                                                <xf:action ev:event="DOMActivate">
                                                    <xf:insert nodeset="instance('save-data')/mods:name" at="index('name-book-reviewed-repeat')" position="before" origin="instance('compact-template')/mods:relatedItem[1]/mods:name[1]"/>
