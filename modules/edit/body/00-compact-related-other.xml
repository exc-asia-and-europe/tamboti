                                        <xf:trigger>
                                            <xf:label>+</xf:label>
                                            <xf:action ev:event="DOMActivate">
                                                <xf:insert nodeset="instance('save-data')/mods:part" at="index('related-item-part-repeat')" position="before" origin="instance('compact-template')/mods:part"/>
