                                                    <xf:input ref="./mods:number" class="text_10">
                                                        <xf:label>
                                                            <xf:output ref="./../@type"/>
                                                        </xf:label>
                                                    </xf:input>
                                                </xf:group>
                                            </xf:repeat>
                                        </fieldset>
                                    </xf:group>
                                    <xf:group ref=".[mods:date and (instance('save-data')/mods:extension/ext:template='related-article-in-periodical-latin' or instance('save-data')/mods:extension/ext:template='related-article-in-periodical-transliterated' or instance('save-data')/mods:extension/ext:template='related-book-review-in-periodical-latin'or instance('save-data')/mods:extension/ext:template='related-book-review-in-periodical-transliterated' or instance('save-data')/mods:extension/ext:template='related-newspaper-article-latin' or instance('save-data')/mods:extension/ext:template='related-newspaper-article-transliterated')]">
