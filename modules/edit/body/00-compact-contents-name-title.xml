						                <xf:output value="'Global Language'">
						                    <xf:hint ref="id('hint-code_lang-global-compact', instance('code-tables'))/help"/>
						                </xf:output>
                                    </xf:label>
                                    <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='language-3-type-sorted-short-code']/items/item">
