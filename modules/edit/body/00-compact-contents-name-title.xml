                                            <xf:select1 ref="/mods:mods/mods:extension/ext:transliterationOfResource">
                                                <xf:label>
									                <xf:output value="'Global Transliteration'">
									                    <xf:hint ref="id('hint-code_transliteration-global-compact', instance('code-tables'))/help"/>
									                </xf:output>
                                                </xf:label>
                                                <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='transliteration-short-code']/items/item">
                                                    <xf:label ref="label"/>
                                                    <xf:value ref="value"/>
                                                </xf:itemset>
                                            </xf:select1>
