									                <xf:output value="'Local Transliteration'">
									                    <xf:hint ref="id('hint-code_transliteration-local-compact', instance('code-tables'))/help"/>
									                </xf:output>
                                                </xf:label>
                                                <xf:itemset nodeset="instance('code-tables')/code-table[code-table-name='transliteration-short-code']/items/item">
                                                    <xf:label ref="label"/>
                                                    <xf:value ref="value"/>
                                                </xf:itemset>
                                            </xf:select1>
